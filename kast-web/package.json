    "history": "4.10.1",
    "i18next": "^17.0.18",
    "i18next-browser-languagedetector": "^4.0.2",
    "material-ui-chip-input": "^1.1.0",
    "material-ui-dropzone": "^3.3.0",
    "mediasoup-client": "^3.6.5",
    "node-sass": "4.14.1",
